# reading_list.py

book=['Harry Potter',
'1984',
'The Fault in Our Stars',
'The Mom Test',
'Life in Code']
print(book)
book.append('Pachinko')
print(book)
book.remove('The Fault in Our Stars')
print(book)
book.pop(1)
print(book)